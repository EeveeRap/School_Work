{
    "extends": "eslint:recommended",
    "rules": {
        /*"no-plusplus": "error",*/
        "semi": [
            "error",
            "always"
        ],
        // "quotes": [
        //     "error",
        //     "single"
        // ],
        "no-var": "error",
        "eqeqeq": "error",
        "wrap-iife": "error",
        "strict": "error",
        "default-param-last": "error"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "commonjs": true
    },
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    }
}